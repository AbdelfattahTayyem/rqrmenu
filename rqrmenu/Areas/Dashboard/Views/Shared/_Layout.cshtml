        <!-- Global Required Scripts Start -->
        <script src="~/assetsdashboard/js/jquery-3.5.0.min.js"></script>
        <script src="~/assetsdashboard/js/popper.min.js"></script>
        <script src="~/assetsdashboard/js/bootstrap.min.js"></script>
        <script src="~/assetsdashboard/js/perfect-scrollbar.js">
        </script>
        <script src="~/assetsdashboard/js/jquery-ui.min.js">
        </script>
        <!-- Global Required Scripts End -->
